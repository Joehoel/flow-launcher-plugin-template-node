{
	"name": "flow-launcher-plugin-template-node",
	"description": "Flow Launcher plugin template for Node.js",
	"main": "./dist/main.js",
	"scripts": {
		"dev": "tsc --watch",
		"build": "tsc",
		"lint": "eslint src/**/*.ts",
		"format": "eslint src/**/*.ts --fix",
		"prepare": "husky install"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint"
		}
	},
	"devDependencies": {
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"eslint": "^8.34.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.1.2",
		"prettier": "^2.8.4",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"open": "^8.4.0",
		"winston": "^3.8.2",
		"zod": "^3.20.6"
	}
}
